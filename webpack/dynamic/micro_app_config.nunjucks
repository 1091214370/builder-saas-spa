const microAppName = '{{ appName }}';
const pages = {{ pages }};

const modules = [];
Object.keys(pages).forEach(item => {
  if (item !== 'common') {
    modules.push({
      ...pages[item],
      moduleName: item,
      getComponent: () => System.import(PUBLIC_PATH + item + '.js'),
    });
  }
});

export default {
  microAppName,
  modules,
};
